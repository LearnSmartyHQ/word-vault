	"definitions": [
		{
			"senseNumber": 1,
			"definition": "to think or guess that the amount, cost or size of something is smaller than it really is",
			"sensetop": "underestimate somethingunderestimate what, how, etc…",
			"cefr": "c1",
			"examples": [
				{
					"text": "to underestimate the cost of the project",
					"contextForm": "underestimate something"
				},
				{
					"text": "We underestimated the time it would take to get there."
				},
				{
					"text": "The statistics seriously underestimate the number of people affected."
				},
				{
					"text": "We underestimated how long it would take.",
					"contextForm": "underestimate what, how, etc…"
				}
			],
			"topics": ["Doubt, guessing and certainty"],
			"collocations": {
				"adverb": ["badly", "considerably", "greatly"],
				"verb + underestimate": ["tend to"],
				"phrases": ["something should not be underestimated"]
			}
		},
		{
			"senseNumber": 2,
			"definition": "to not realize how good, strong, determined, difficult, etc. somebody/something really is",
			"sensetop": "underestimate somebody/somethingunderestimate what, how, etc…",
			"examples": [
				{
					"text": "Never underestimate your opponent.",
					"contextForm": "underestimate somebody/something"
				},
				{
					"text": "Their ability should not be underestimated."
				},
				{
					"text": "Don't underestimate how difficult this is going to be.",
					"contextForm": "underestimate what, how, etc…"
				},
				{
					"text": "Don't underestimate what she is capable of."
				},
				{
					"text": "He realized that he had seriously underestimated their strength."
				},
				{
					"text": "The importance of these feelings should not be underestimated."
				},
				{
					"text": "We consistently underestimate the resources and motivation of our adversaries."
				}
			],
			"collocations": {
				"adverb": ["badly", "considerably", "greatly"],
				"verb + underestimate": ["tend to"],
				"phrases": ["something should not be underestimated"]
			}
		}
