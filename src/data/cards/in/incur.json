	"definitions": [
		{
			"senseNumber": 1,
			"definition": "if you incur something unpleasant, you are in a situation in which you have to deal with it",
			"sensetop": "incur something",
			"cefr": "c1",
			"examples": [
				{
					"text": "She had incurred the wrath of her father by marrying without his consent."
				}
			]
		},
		{
			"senseNumber": 2,
			"definition": "if you incur costs, you have to pay them",
			"sensetop": "incur something",
			"cefr": "c1",
			"examples": [
				{
					"text": "You risk incurring bank charges if you exceed your overdraft limit."
				}
			]
		}
	],
