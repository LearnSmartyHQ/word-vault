  "definitions": [
    {
      "senseNumber": 1,
      "definition": "to warn somebody about a dangerous situation or one that requires immediate action",
      "sensetop": "alert somebody(be) alerted by somethingbe alerted that…",
      "cefr": "c1",
      "examples": [
        {
          "text": "Neighbours quickly alerted the emergency services.",
          "contextForm": "alert somebody"
        },
        {
          "text": "Alerted by a noise downstairs, he sat up and turned on the light.",
          "contextForm": "(be) alerted by something"
        },
        {
          "text": "The coastguard were alerted that a yacht had run into difficulties.",
          "contextForm": "be alerted that…"
        }
      ],
      "topics": ["Danger", "War and conflict"]
    },
    {
      "senseNumber": 2,
      "definition": "to make somebody aware of something",
      "sensetop": "be alerted to something",
      "cefr": "c1",
      "examples": [
        {
          "text": "They had been alerted to the possibility of further price rises.",
          "contextForm": "be alerted to something"
        }
      ]
    }
  ],
