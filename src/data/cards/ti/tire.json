	"definitions": [
		{
			"senseNumber": null,
			"definition": "to become tired and feel as if you want to sleep or rest; to make somebody feel this way",
			"sensetop": "tire (somebody)",
			"examples": [
				{
					"text": "Her legs were beginning to tire."
				},
				{
					"text": "He has made a good recovery but still tires easily."
				},
				{
					"text": "Walking even a short distance tires him."
				},
				{
					"text": "She found herself tiring more quickly these days."
				},
				{
					"text": "Long conversations tired her."
				}
			],
			"topics": ["Feelings"],
			"collocations": {
				"adverb": ["easily", "quickly", "eventually"],
				"verb + tire": ["begin to"]
			}
		},
		{
			"senseNumber": null,
			"definition": "to do something a lot, especially in a way that annoys people",
			"examples": [
				{
					"text": "He went to Harvard—as he never tires of reminding us."
				}
			]
		}
	],
