  "definitions": [
    {
      "senseNumber": null,
      "definition": "to bring a feeling, a memory or an image into your mind",
      "sensetop": "evoke something",
      "examples": [
        {
          "text": "The music **evoked memories** of her youth."
        },
        {
          "text": "His case is unlikely to evoke public sympathy."
        },
        {
          "text": "Her face, though sad, still evoked a feeling of serenity."
        },
        {
          "text": "The novel vividly evokes the life of the Irish in Australia."
        },
        {
          "text": "products that help evoke an old-fashioned mood"
        },
        {
          "text": "the actor's ability to evoke a variety of emotions"
        }
      ],
      "collocations": {
        "adverb": ["beautifully", "clearly", "effectively"],
        "verb + evoke": ["attempt to", "seek to", "try to"],
        "evoke + noun": [
          "the ability to evoke something",
          "the power to evoke something"
        ]
      }
    }
