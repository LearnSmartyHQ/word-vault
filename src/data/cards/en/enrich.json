  "definitions": [
    {
      "senseNumber": 1,
      "definition": "to improve the quality of something, often by adding something to it",
      "sensetop": "enrich something",
      "cefr": "c1",
      "examples": [
        {
          "text": "The study of science has enriched all our lives."
        },
        {
          "text": "Reading good literature can enrich all our lives."
        },
        {
          "text": "The educational experience of the group is enriched by contact with Spanish speakers."
        }
      ]
    },
    {
      "senseNumber": 2,
      "definition": "to improve the quality of food by adding vitamins or nutrients",
      "sensetop": "enrich something (with something)",
      "examples": [
        {
          "text": "Most breakfast cereals are enriched with vitamins."
        }
      ]
    },
    {
      "senseNumber": 3,
      "definition": "to make somebody/something rich or richer",
      "sensetop": "enrich somebody/something",
      "examples": [
        {
          "text": "a nation enriched by oil revenues"
        },
        {
          "text": "He used his position to enrich himself."
        }
      ]
    }
  ],
