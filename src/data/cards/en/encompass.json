	"definitions": [
		{
			"senseNumber": 1,
			"definition": "to include a large number or range of things",
			"sensetop": "encompass something",
			"cefr": "c1",
			"examples": [
				{
					"text": "The job encompasses a wide range of responsibilities."
				},
				{
					"text": "The group encompasses all ages."
				}
			]
		},
		{
			"senseNumber": 2,
			"definition": "to surround or cover something completely",
			"sensetop": "encompass something",
			"examples": [
				{
					"text": "The fog soon encompassed the whole valley."
				}
			]
		}
	],
