	"definitions": [
		{
			"senseNumber": 1,
			"definition": "to fill a hole with a substance or piece of material that fits tightly into it",
			"sensetop": "plug something (up)",
			"cefr": "c1",
			"examples": [
				{
					"text": "He plugged the hole in the pipe with an old rag."
				},
				{
					"text": "Divers succeeded in plugging the leaks from the tanker."
				}
			]
		},
		{
			"senseNumber": 2,
			"definition": "to provide something that has been missing from a particular situation and is needed in order to improve it",
			"sensetop": "plug something",
			"cefr": "c1",
			"examples": [
				{
					"text": "A cheaper range of products was introduced to **plug the gap** at the lower end of the market."
				}
			]
		},
		{
			"senseNumber": 3,
			"definition": "to give praise or attention to a new book, film, etc. in order to encourage people to buy it or see it",
			"sensetop": "plug something",
			"labels": "(informal)",
			"cefr": "c2",
			"examples": [
				{
					"text": "She came on the show to plug her latest album."
				}
			],
			"topics": ["Film and theatre"]
		},
		{
			"senseNumber": 4,
			"definition": "to shoot somebody",
			"sensetop": "plug somebody",
			"labels": "(North American English, old-fashioned, informal)",
			"examples": []
		}
	],
