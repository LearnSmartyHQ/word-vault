      "labels": "(figurative)",
      "cefr": "c2",
      "examples": [
        {
          "text": "She shrieked in fright.",
          "contextForm": "shriek (in something)"
        },
        {
          "text": "The audience was shrieking with laughter.",
          "contextForm": "shriek with something"
        },
        {
          "text": "He all but shrieked when he saw her."
        },
        {
          "text": "The answer shrieked at her *(= was very obvious)*."
        }
      ],
      "topics": ["Feelings"],
