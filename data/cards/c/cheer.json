  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to shout loudly, in order to show support or praise for somebody, or to encourage them",
      "sensetop": "cheer for somebody/somethingcheer somebody",
      "cefr": "b2",
      "examples": [
        {
          "text": "We all cheered as the team came onto the field."
        },
        {
          "text": "Cheering crowds greeted their arrival."
        },
        {
          "text": "The fans cheered for their team.",
          "contextForm": "cheer for somebody/something"
        },
        {
          "text": "The crowd cheered the president as he drove slowly by.",
          "contextForm": "cheer somebody"
        },
        {
          "text": "The crowd cheered loudly as he came on stage."
        },
        {
          "text": "The crowd cheered loudly as the goalkeeper made a brilliant save."
        }
      ],
      "collocations": {
        "adverb": ["loudly", "wildly"],
        "preposition": ["for"]
      }
    },
    {
      "senseNumber": "2",
      "definition": "to encourage somebody or to give them hope or comfort",
      "sensetop": "be cheered (by something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "She was cheered by the news from home.",
          "contextForm": "be cheered (by something)"
