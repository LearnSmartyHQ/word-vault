  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to communicate with somebody, especially while you work, play or spend time with them",
      "sensetop": "interact (with somebody)",
      "cefr": "b2",
      "examples": [
        {
          "text": "Teachers have a limited amount of time to interact with each child."
        },
        {
          "text": "He interacts very well with other children."
        },
        {
          "text": "The only thing he interacts with is his computer!"
        }
      ],
      "collocations": {
        "adverb": ["well", "socially"],
        "preposition": ["with"]
      }
    },
    {
      "senseNumber": "2",
      "definition": "if one thing interacts with another, or if two things interact, the two things have an effect on each other",
      "sensetop": "interact (with something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "Perfume interacts with the skin's natural chemicals."
        },
        {
          "text": "This hormone interacts closely with other hormones in the body."
        },
        {
          "text": "These devices allow the robot to physically interact with its environment."
        }
      ],
      "collocations": {
        "adverb": ["closely", "directly", "physically"],
        "preposition": ["with"]
      }
    }
