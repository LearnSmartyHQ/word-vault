  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to remove or reduce an unpleasant feeling or pain",
      "sensetop": "relieve something",
      "cefr": "b2",
      "examples": [
        {
          "text": "to relieve the symptoms of a cold"
        },
        {
          "text": "to **relieve anxiety/guilt/stress**"
        },
        {
          "text": "Being able to tell the truth at last seemed to relieve her."
        },
        {
          "text": "They try to relieve the symptoms of depression by drinking."
        },
        {
          "text": "Don't resort to alcohol to relieve stress."
        },
        {
          "text": "Take painkillers and hot drinks to relieve the symptoms."
        }
      ],
