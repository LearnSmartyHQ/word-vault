  "definitions": [
    {
      "senseNumber": 1,
      "definition": "to make a position of power or success stronger so that it is more likely to continue",
      "sensetop": "consolidate (something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "With this new movie he has **consolidated** his **position** as the country's leading director."
        },
        {
          "text": "Italy consolidated their lead with a second goal."
        }
      ],
      "topics": ["Success"]
    },
    {
      "senseNumber": 2,
      "definition": "to join things together into one; to be joined into one",
      "sensetop": "consolidate (something)",
      "labels": "(specialist)",
      "cefr": "c1",
      "examples": [
        {
          "text": "All the debts have been consolidated."
        },
        {
          "text": "consolidated accounts"
        },
        {
          "text": "The two companies consolidated for greater efficiency."
        }
      ],
      "topics": ["Money"]
    }
  ],
