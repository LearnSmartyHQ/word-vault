  "definitions": [
    {
      "senseNumber": 1,
      "definition": "to work together with somebody in order to produce or achieve something",
      "sensetop": "collaborate (with somebody) (on something)collaborate (with somebody) (in something/in doing something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "Researchers around the world are collaborating to develop a new vaccine."
        },
        {
          "text": "We have collaborated on many projects over the years.",
          "contextForm": "collaborate (with somebody) (on something)"
        },
        {
          "text": "She agreed to collaborate with him in writing the biography of her grandmother.",
          "contextForm": "collaborate (with somebody) (in something/in doing something)"
        }
      ],
      "topics": ["Discussion and agreement"],
      "collocations": {
        "adverb": ["actively", "closely"],
        "preposition": ["on", "with"]
      }
    },
    {
      "senseNumber": 2,
      "definition": "to help the enemy who has taken control of your country during a war",
      "sensetop": "collaborate (with somebody)",
      "labels": "(disapproving)",
      "cefr": "c1",
      "examples": [
        {
          "text": "He was accused of collaborating with the enemy."
        }
      ]
    }
  ],
