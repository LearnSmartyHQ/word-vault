  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to support something that you think is right and make sure that it continues to exist",
      "sensetop": "uphold something",
      "cefr": "c1",
      "examples": [
        {
          "text": "We have a duty to uphold the law."
        },
        {
          "text": "The regime has been criticized for failing to uphold human rights."
        },
        {
          "text": "We are determined to uphold the law."
        }
      ],
      "collocations": {
        "adverb": ["consistently", "firmly", "rigorously"],
        "verb + uphold": ["have a duty to", "be determined to", "seek to"]
      }
    },
    {
      "senseNumber": "2",
      "definition": "to agree that a previous decision was correct or that a request is reasonable",
      "sensetop": "uphold something",
      "cefr": "c1",
      "examples": [
        {
          "text": "to **uphold a conviction/an appeal/a complaint**"
        },
        {
          "text": "Three judges unanimously upheld the sentence."
        },
        {
          "text": "The Press Council refused to uphold the complaint."
        },
        {
          "text": "The conviction was upheld by the Court of Appeal."
        }
      ],
      "topics": ["Preferences and decisions", "Law and justice"],
      "collocations": {
