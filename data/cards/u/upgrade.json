  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to make a machine, computer system, etc. more powerful and efficient; to start using a new and better version of a machine, system, etc.",
      "sensetop": "upgrade somethingupgrade (from something) (to something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "We are constantly upgrading our software to meet customers' needs.",
          "contextForm": "upgrade something"
        },
        {
          "text": "customers who want to upgrade from version 4.2 to version 4.5",
          "contextForm": "upgrade (from something) (to something)"
        },
        {
          "text": "Users can easily upgrade to the new version."
        }
      ],
      "topics": ["Computers"],
      "collocations": {
        "adverb": ["significantly", "substantially", "constantly"],
        "preposition": ["from", "to", "with"]
      }
    },
    {
      "senseNumber": "2",
      "definition": "to improve the condition of a building, etc. in order to provide a better service",
      "sensetop": "upgrade something",
      "cefr": "c1",
      "examples": [
        {
          "text": "to upgrade the town’s leisure facilities"
        },
        {
          "text": "The towpath was upgraded for use by cyclists."
        }
      ]
    },
    {
      "senseNumber": "3",
      "definition": "to give somebody a better seat on a plane, room in a hotel, etc. than the one that they have paid for",
