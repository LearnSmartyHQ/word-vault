  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to remove a cover or curtain from a painting, statue, etc. so that it can be seen in public for the first time",
      "sensetop": "unveil something",
      "cefr": "c1",
      "examples": [
        {
          "text": "The Queen unveiled a plaque to mark the official opening of the hospital."
        }
      ],
      "collocations": {
        "adverb": ["formally", "officially", "just"]
      }
    },
    {
      "senseNumber": "2",
      "definition": "to show or introduce a new plan, product, etc. to the public for the first time",
      "sensetop": "unveil something",
      "cefr": "c1",
      "examples": [
        {
          "text": "They will be unveiling their new models at the Motor Show."
        },
        {
          "text": "The government has unveiled plans for new energy legislation."
        }
      ],
      "synonyms": "reveal",
      "collocations": {
        "adverb": ["formally", "officially", "just"]
      }
    }
