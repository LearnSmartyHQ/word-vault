  "definitions": [
    {
      "senseNumber": 1,
      "definition": "to fix something in a substance or solid object",
      "sensetop": "be embedded in somethingembed something/itself (in something)",
      "labels": "(figurative)",
      "cefr": "c1",
      "examples": [
        {
          "text": "an operation to remove glass that was embedded in his leg",
          "contextForm": "be embedded in something"
        },
        {
          "text": "These attitudes are deeply embedded in our society *(= felt very strongly and difficult to change)*."
        },
        {
          "text": "The bullet embedded itself in the wall.",
          "contextForm": "embed something/itself (in something)"
        }
      ]
    },
    {
      "senseNumber": 2,
      "definition": "to send a journalist, photographer, etc. to an area where there is fighting, so that they can travel with the army and report what is happening",
      "sensetop": "embed somebody",
      "cefr": "c2",
      "examples": [
        {
          "text": "embedded reporters in the war zone"
        }
      ],
      "topics": ["TV, radio and news"]
    },
    {
      "senseNumber": 3,
      "definition": "to place a sentence inside another sentence. In the sentence ‘I’m aware that she knows’, she knows is an embedded sentence.",
      "sensetop": "embed something",
      "labels": "(linguistics)",
      "examples": []
    }
  ],
