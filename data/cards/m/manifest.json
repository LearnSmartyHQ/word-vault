  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to show something clearly, especially a feeling, an attitude or a quality",
      "sensetop": "manifest something (in something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "Social tensions were manifested in the recent political crisis."
        }
      ]
    },
    {
      "senseNumber": "2",
      "definition": "to appear or become easy to notice",
      "sensetop": "manifest itself (in something)",
      "cefr": "c1",
      "examples": [
        {
          "text": "The symptoms of the disease manifested themselves ten days later."
        }
      ],
      "synonyms": "appear"
    }
  ],
  "wordOrigin": "verb late Middle English: via Old French from Latin manifestus."
