  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to move or act quickly because there is not much time",
      "sensetop": "hurry to do something",
      "cefr": "b1",
      "ox3000": true,
      "examples": [
        {
          "text": "You'll have to hurry if you want to catch that train."
        },
        {
          "text": "We need to hurry or we'll be late for school."
        },
        {
          "text": "The kids hurried to open their presents.",
          "contextForm": "hurry to do something"
        }
      ],
      "synonyms": "rush"
    },
    {
