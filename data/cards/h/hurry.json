      "cefr": "b1",
      "ox3000": true,
      "examples": [
        {
          "text": "He picked up his bags and hurried across the courtyard."
        },
        {
          "text": "She hurried away without saying goodbye."
        },
        {
          "text": "She hurried after Gary to apologize to him."
        },
        {
          "text": "She hurried down the street."
        },
        {
          "text": "I hurried home to tell him the news."
        },
        {
          "text": "Ben hurried after her."
        },
        {
          "text": "She came hurrying over to greet them."
        },
        {
          "text": "She hurried along the corridor."
        },
        {
          "text": "We nodded at her and hurried out the door."
        },
        {
          "text": "He hurried over to the window to see what the noise was."
        },
        {
          "text": "Kathy hurried off in the same direction."
        }
