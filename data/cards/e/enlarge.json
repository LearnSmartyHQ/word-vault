  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to make something bigger; to become bigger",
      "sensetop": "enlarge (something)",
      "examples": [
        {
          "text": "There are plans to enlarge the recreation area."
        },
        {
          "text": "Reading will enlarge your vocabulary."
        },
        {
          "text": "The castle was enlarged considerably in the fifteenth century."
        },
        {
          "text": "The gallery is seeking to enlarge its holdings of Danish art."
        },
        {
          "text": "The little blisters enlarge and eventually burst to form ulcers."
        }
      ],
