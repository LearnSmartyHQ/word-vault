  "definitions": [
    {
      "senseNumber": "1",
      "definition": "to express or represent an idea or a quality",
      "sensetop": "embody somethingbe embodied in something",
      "cefr": "c1",
      "examples": [
        {
          "text": "a politician who embodied the hopes of black youth",
          "contextForm": "embody something"
        },
        {
          "text": "the principles embodied in the Declaration of Human Rights",
          "contextForm": "be embodied in something"
        },
        {
          "text": "These ideals were embodied in the constitution."
        },
        {
          "text": "The new constitution embodied the right to free speech."
        },
        {
          "text": "We want to build a national team that embodies competitive spirit and skill."
        }
      ],
      "synonyms": "represent"
    },
    {
      "senseNumber": "2",
      "definition": "to include or contain something",
      "sensetop": "embody something",
      "labels": "(formal)",
      "examples": [
        {
          "text": "This model embodies many new features."
        }
      ]
    }
  ],
  "wordOrigin": "mid 16th cent.: from em- + body, on the pattern of Latin incorporare."
